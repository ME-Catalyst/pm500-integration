{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Node-RED flow export",
  "type": "array",
  "minItems": 1,
  "items": {
    "type": "object",
    "required": ["id", "type"],
    "properties": {
      "id": {
        "type": "string",
        "minLength": 1
      },
      "type": {
        "type": "string",
        "minLength": 1
      },
      "z": {
        "type": "string",
        "minLength": 1
      },
      "wires": {
        "type": "array",
        "items": {
          "type": "array",
          "items": {
            "type": "string",
            "minLength": 1
          }
        }
      }
    },
    "additionalProperties": true
  }
}
